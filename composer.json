{
    "name": "pr0ject-x/px-valet",
    "description": "The project-x valet plugin.",
    "type": "px-plugin",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Travis Tomka",
            "email": "underoathosx@gmail.com"
        }
    ],
    "repositories": {
        "px": {
            "type": "path",
