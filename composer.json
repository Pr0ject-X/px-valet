    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpmd/phpmd": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Pr0jectX\\PxValet\\": "./src"
        }
    }
}
